# Example properties file for EnumeratePatternsToJSON
# CoSaTa


#
#   Input and Output Paths
#
# Tablestore
tablestoreIndex = annotation/sync-tablestore/tableindex.txt

# EntailmentBank-based manual queries for PeterC
IMLInputPath = examples/enumeration/worldtree/
IMLOutputPath = output/enumeration/worldtree/


#
#   Parameters
#

# Maximum number of enumerated solutions per pattern
maxMatches = 10000

# Number of threads to use for the outer loop of the solver (i.e. number of patterns to evaluate in parallel)
numThreads = 8

#
#   Additional exports
#

# Export a human-readable HTML log of inference pattern solutions (limited to a certain number, to keep the log size small)
exportInfPatHTML = true

# Export a copy of the tablestore in HTML format
exportTableStoreHTML = true

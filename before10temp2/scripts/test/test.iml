	row som = [TABLE:"KINDOF", HYPONYM:<SOM>, HYPERNYM:"state of matter"]
	row propSomTemp = [TABLE:"PROP-STATESOFMATTER-TEMPS", STATEOFMATTER:<SOM>]
	
	// Code

endinferencepattern




